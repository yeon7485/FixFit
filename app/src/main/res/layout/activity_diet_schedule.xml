    <!-- 남는 자리 -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="@drawable/title_bar">

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="15dp"
            android:layout_marginVertical="10dp">

            <ImageView
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:background="@drawable/oval_style" />

            <ImageView
                android:layout_width="80dp"
                android:layout_height="60dp"
                android:layout_marginVertical="10dp"
                android:padding="5dp"
                android:src="@drawable/ic_baseline_food_bank_24" />
        </FrameLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:fontFamily="@font/cafe24_ssurround"
            android:gravity="center"
            android:text="식단 기록"
            android:textColor="@color/black"
            android:textSize="27sp" />

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1" />

        <ImageView
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:layout_gravity="right"
            android:layout_marginEnd="5dp"
            android:src="@drawable/ic_logo_fixfit" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/frag_schedule_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:paddingStart="8dp"
            android:paddingTop="4dp"
            android:text="날짜 변경"
            android:textSize="16sp"
            grid:layout_constraintStart_toStartOf="parent"
            grid:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/change"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="end"
            android:layout_margin="5dp"
            android:paddingTop="4dp"
            android:paddingEnd="8dp"
            android:text="날짜 변경"
            android:textSize="16sp"
            grid:layout_constraintEnd_toEndOf="parent"
            grid:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
